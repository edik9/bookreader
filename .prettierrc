{
  "printWidth": 100,           
  "tabWidth": 2,               
  "useTabs": false,           
  "semi": true,               
  "singleQuote": false,        
  "quoteProps": "as-needed",   
  "jsxSingleQuote": false,     
  "trailingComma": "es5",      
  "bracketSpacing": true,      
  "bracketSameLine": false,    
  "arrowParens": "always",     
  "endOfLine": "lf",           
  "proseWrap": "preserve",     
  "importOrder": ["^react", "^@", "^[a-z]", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}